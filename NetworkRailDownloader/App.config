<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <connectionStrings>
    <clear />
    <add name="database" connectionString="CONN STRING HERE" providerName="MySql.Data.MySqlClient"/>
  </connectionStrings>
  <appSettings>
    <add key="ScheduleUri" value="https://datafeeds.networkrail.co.uk/ntrod/CifFileAuthenticate" />
    <add key="ScheduleTimeoutMins" value="5" />
    <add key="ActiveMQConnectionString" value="stomp:tcp://datafeeds.networkrail.co.uk:61618" />
    <add key="ActiveMQDurableClientId" value=""/>
    <add key="ActiveMQDurableSubscriberId" value=""/>
    <add key="Username" value="USERNAMEHERE" />
    <add key="Password" value="PASSWORDHERE" />
    <add key="NMSLogging" value="false"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.serviceModel>
    <bindings>
      <!-- Uses wsHttpBinding-->
      <netTcpBinding>
        <binding name="NetTcpBinding_ICacheService" />
      </netTcpBinding>
    </bindings>
    <client>
      <!-- specifies the endpoint to use when calling the service -->
      <endpoint address="net.tcp://localhost:83/CacheService"
          binding="netTcpBinding" bindingConfiguration="NetTcpBinding_ICacheService"
          contract="TrainNotifier.Common.Services.ICacheService" name="NetTcpBinding_ICacheService">
      </endpoint>
    </client>
  </system.serviceModel>
</configuration>