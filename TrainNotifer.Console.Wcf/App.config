<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="CacheExpiryDays" value="7"/>
  </appSettings>
  <system.serviceModel>
    <services>
      <service behaviorConfiguration="TrainNotifier.Console.Wcf.ServiceBehavior"
        name="TrainNotifier.WcfLibrary.CacheService">
        <endpoint address="" binding="netTcpBinding" bindingConfiguration=""
          contract="TrainNotifier.Common.Services.ICacheService">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <endpoint address="mex" binding="mexTcpBinding" bindingConfiguration=""
          contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="net.tcp://localhost:83/CacheService" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="TrainNotifier.Console.Wcf.ServiceBehavior">
          <serviceMetadata httpGetEnabled="false" />
          <serviceDebug includeExceptionDetailInFaults="true" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
</configuration>