<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <connectionStrings>
    <clear />
    <add name="database" connectionString="CONN STRING HERE" providerName="MySql.Data.MySqlClient"/>
    <add name="archive" connectionString="CONN STRING HERE" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <appSettings>
    <add key="CacheExpiryDaysTM" value="28"/>
    <add key="CacheExpiryDaysStanox" value="0.5"/>
  </appSettings>
  <system.serviceModel>
    <services>
      <service behaviorConfiguration="TrainNotifier.Console.Wcf.ServiceBehavior"
        name="TrainNotifier.WcfLibrary.CacheService">
        <endpoint address="" binding="netTcpBinding" bindingConfiguration=""
          contract="TrainNotifier.Common.Services.ICacheService">
          <identity>
            <dns value="localhost" />
          </identity>
        </endpoint>
        <endpoint address="mex" binding="mexTcpBinding" bindingConfiguration=""
          contract="IMetadataExchange" />
        <host>
          <baseAddresses>
            <add baseAddress="net.tcp://localhost:83/CacheService" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="TrainNotifier.Console.Wcf.ServiceBehavior">
          <serviceMetadata httpGetEnabled="false" />
          <serviceDebug includeExceptionDetailInFaults="true" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.data>
    <DbProviderFactories>
      <clear />
      <add
        name="MySQL Data Provider"
        invariant="MySql.Data.MySqlClient"
        description=".Net Framework Data Provider for MySQL"
        type="MySql.Data.MySqlClient.MySqlClientFactory, MySql.Data, Version=6.5.4.0, Culture=neutral, PublicKeyToken=c5687fc88969c44d" />
    </DbProviderFactories>
  </system.data>
</configuration>